{
  "name": "subscription-service-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["subscription-service"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "subscription-service-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["subscription-service:build", "subscription-service:serve"]
    }
  }
}
