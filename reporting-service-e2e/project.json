{
  "name": "reporting-service-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["reporting-service"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "reporting-service-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["reporting-service:build", "reporting-service:serve"]
    }
  }
}
