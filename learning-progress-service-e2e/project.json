{
  "name": "learning-progress-service-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["learning-progress-service"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "learning-progress-service-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "learning-progress-service:build",
        "learning-progress-service:serve"
      ]
    }
  }
}
